{
    "name": "awesomeChatRoom",
    "description": "awesomeChatRoom chat web app",
    "authors": [
        "Romain Laneuville"
    ],
    "license": "MIT",
    "keywords": [
        "awesomeChatRoom",
        "websocket",
        "services",
        "chat"
    ],
    "homepage": "https://github.com/ZiperRom1/awesomeChatRoom",
    "moduleType": [
        "amd"
    ],
    "dependencies": {
        "jquery": "^2.2.0",
        "lodash": "^4.9.0",
        "bootstrap": "^3.3.6",
        "bootstrap-select": "latest",
        "bootstrap-switch": "latest",
        "requirejs": "latest",
        "domReady": "latest",
        "jasny-bootstrap": "latest"
    },
    "overrides": {
        "bootstrap-switch": {
          "main": [
                "src/less/bootstrap3/bootstrap-switch.less",
                "dist/js/bootstrap-switch.js"
            ]
        },
        "jasny-bootstrap": {
            "main": [
                "dist/js/jasny-bootstrap.js",
                "less/*.less"
            ]
        }
    },
    "ignore": [
        "**/.*",
        "node_modules",
        "bower_components",
        "test",
        "tests"
    ]
